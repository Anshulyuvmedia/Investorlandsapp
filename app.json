{
  "expo": {
    "name": "investorland",
    "slug": "InvestorLand",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/mainlogo.jpeg",
    "scheme": "investorland",
    "entryPoint": "expo-router/entry",
    "host": "localhost",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.investor.investorland",
      "associatedDomains": [
        "applinks:your-server.com",
        "applinks:your-server.com"
      ],
      "runtimeVersion": {
        "policy": "appVersion"
      }
    },
    "android": {
      "package": "com.investor.investorland",
      "permissions": [
        "ACCESS_FINE_LOCATION",
        "ACCESS_COARSE_LOCATION"
      ],
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/investorIcon.png",
        "backgroundColor": "#ffffff"
      },
      "runtimeVersion": "1.0.0"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/investorIcon.png"
    },
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/mainlogo.jpeg",
          "resizeMode": "cover",
          "backgroundColor": "#ffffff",
          "enableFullScreenImage_legacy": true
        }
      ],
      [
        "expo-font",
        {
          "fonts": [
            "./assets/fonts/Rubik-Bold.ttf",
            "./assets/fonts/Rubik-ExtraBold.ttf",
            "./assets/fonts/Rubik-Medium.ttf",
            "./assets/fonts/Rubik-Light.ttf",
            "./assets/fonts/Rubik-Regular.ttf",
            "./assets/fonts/Rubik-SemiBold.ttf"
          ]
        }
      ],
      [
        "expo-video",
        {
          "supportsBackgroundPlayback": true,
          "supportsPictureInPicture": true
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "b9b062d0-380c-4ab0-8b80-cb450a9dc58a"
      },
      "GOOGLE_MAPS_API_KEY": "AIzaSyA_KLtDmYZB0Qy_b0f6LHJSlDV2wYYPf8s",
      "ANDROID_CLIENT_ID": "674185229942-hkqcgtqbjja7gh5ilnb1t8ed7t1pu72t.apps.googleusercontent.com",
      "WEB_CLIENT_ID": "674185229942-u0ueij6dphapn8a8bacgtnmm1omoricq.apps.googleusercontent.com"
    },
    "owner": "anshulyuvmedia",
    "updates": {
      "url": "https://u.expo.dev/b9b062d0-380c-4ab0-8b80-cb450a9dc58a"
    }
  }
}
